using System;

namespace 코드의_흐름제어_연습문제2
{
    class Program
    {
        static void Main(string[] args)
        {
            for(int i=0; i<=4; i++)
            {
               for(int j=0; j<=i; j++)
                {
                    Console.Write("*");
                }
                Console.WriteLine();
                
            }
        }
    }
}
